<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>

  <listener>
  	<listener-class>com.suji.lsnr.SessionCheck</listener-class>
  </listener>
  <servlet>
  	<servlet-name>LoginCtrl</servlet-name>
  	<display-name>LoginCtrl</display-name>
  	<description></description>
  	<servlet-class>com.suji.ctrl.LoginCtrl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LogoutCtrl</servlet-name>
  	<display-name>LogoutCtrl</display-name>
  	<description></description>
  	<servlet-class>com.suji.ctrl.LogoutCtrl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PostQuote</servlet-name>
  	<display-name>PostQuote</display-name>
  	<description></description>
  	<servlet-class>com.suji.ctrl.PostQuote</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>SignupCtrl</servlet-name>
  	<display-name>SignupCtrl</display-name>
  	<description></description>
  	<servlet-class>com.suji.ctrl.SignupCtrl</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LoginCtrl</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LogoutCtrl</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PostQuote</servlet-name>
  	<url-pattern>/postQuote</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SignupCtrl</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
</web-app>
