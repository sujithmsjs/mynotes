<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SignUpCtrl</servlet-name>
  	<display-name>SignUpCtrl</display-name>
  	<description></description>
  	<servlet-class>com.suji.ctrl.SignUpCtrl</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ShowPromise</servlet-name>
  	<display-name>ShowPromise</display-name>
  	<description></description>
  	<servlet-class>com.suji.ctrl.ShowPromise</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Exp1</servlet-name>
  	<display-name>Exp1</display-name>
  	<description></description>
  	<servlet-class>com.suji.prac.exp1.Exp1</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SignUpCtrl</servlet-name>
  	<url-pattern>/signup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ShowPromise</servlet-name>
  	<url-pattern>/ShowPromise</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Exp1</servlet-name>
  	<url-pattern>/Exp1</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
  	<welcome-file>home.html</welcome-file>
  </welcome-file-list>
</web-app>
